<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>MadDogsTattoo</title>
    <link rel="icon" href="images/maddogstattoo-logo.png" type="image/icon type">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Space+Mono&display=swap" rel="stylesheet">
    <link href="lib/fontawesome-free-5.15.2-web/css/all.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.8.2/css/lightbox.min.css">
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-F0G0PVNKZH"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'G-F0G0PVNKZH');
    </script>
</head>
<body>
    <header style="top: 0;">
        <partial name="_Culture" />
        <div class="container">
            <div class="justify-content-between">
                <ul class="justify-content-around text-center">
                    <li class="">
                        <a class="navbar-brand" href="http://maddogstattoo.com"><img src="~/images/maddogstattoo-logo.png" width="200px;" alt="Logo" /></a>
                    </li>
                </ul>
            </div>
        </div>
    </header>
    <div class="">
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>
    <footer class=" footer text-white-50" style="background-color: black;">
        <div class="container text-center">
            @{
            var year = DateTime.Now.Year;
            }
            &copy; @year - Mad Dogs Tattoo - Developed by <a href="http://nemanjapakaski.com">NP</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/fontawesome-free-5.15.2-web/js/all.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
